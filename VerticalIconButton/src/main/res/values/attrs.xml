<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="VerticalIconBoxedButton">
        <attr name="button_text" format="string" />
        <attr name="icon" format="reference" />
        <attr name="button_background_color_normal" format="color" />
        <attr name="icon_background_color" format="color" />
        <attr name="stroke_width" format="integer" />
        <attr name="stroke_color" format="color" />
        <attr name="button_text_color" format="color" />
        <attr name="button_text_size" format="dimension" />
        <attr name="icon_weight" format="integer"/>
        <attr name="icon_margin_top" format="dimension"/>
    </declare-styleable>

    <declare-styleable name="VerticalIconButton">
        <attr name="n_button_text" format="string" />
        <attr name="n_icon" format="reference" />
        <attr name="n_button_background_color_normal" format="color" />
        <attr name="n_stroke_width" format="integer" />
        <attr name="n_stroke_color" format="color" />
        <attr name="n_button_text_color" format="color" />
        <attr name="n_button_text_size" format="dimension" />
        <attr name="n_icon_weight" format="integer"/>
        <attr name="n_icon_margin_top" format="dimension"/>
    </declare-styleable>
</resources>